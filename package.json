{
  "name": "claimer",
  "productName": "Claimer",
  "version": "1.0.0",
  "description": "Claimer",
  "main": "./",
  "scripts": {
    "start": "concurrently npm:dev:*",
    "dev:serve": "nodemon --watch ./backend/ --exec \"clear && node ./backend/index.js\"",
    "dev:run": "nodemon --watch ./src/ --exec \"clear && electron ./src/main.js\"",

    "build": "npm run build:cp && npm run build:build && npm run build:rm",
    "build:build": "electron-packager ./src/ --overwrite --asar --icon ./src/public/img/icon.ico --out ./bin",
    "build:cp": "cp package.json src/package.json",
    "build:rm": "rm src/package.json"
  },
  "keywords": [],
  "author": "Jleguim",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.2.2",
    "electron": "^19.0.7",
    "electron-packager": "^15.5.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.2",
    "superagent": "^8.0.0"
  }
}
